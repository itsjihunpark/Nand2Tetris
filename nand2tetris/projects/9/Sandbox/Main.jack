/** Initializes a new Square game and starts running it. */
class Main {
    function void main() {
        var int charScreenRow;
        var int cursorPosition;

        let charScreenRow = 23;
        let cursorPosition = 0;

        while (cursorPosition<charScreenRow){
            do Output.moveCursor(cursorPosition,0);
            let cursorPosition = cursorPosition + 1;
            do Output.printString("AHHHH");
            do Screen.setColor(true);
            do Screen.drawRectangle(400, 128, 422,140);
            do Sys.wait(5);
            do Screen.setColor(false);
            do Screen.drawRectangle(400, 128, 422,140);
        }
        let cursorPosition = cursorPosition - 1;
        while (cursorPosition>-1){
            do Output.moveCursor(cursorPosition,0);
            let cursorPosition = cursorPosition - 1;
            do Output.printString("E");
        }
        return;
    }
}